{
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, UseTab: Always, IndentWidth: 4, TabWidth: 4, AllowShortIfStatementsOnASingleLine: false, IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4, NamespaceIndentation: All, FixNamespaceComments: false }",
  "files.associations": {
    "*.h": "c",
    "*.c": "c",
    "cmath": "c",
    "system_error": "c",
    "functional": "c",
    "array": "c",
    "tuple": "c",
    "type_traits": "c",
    "utility": "c",
    "random": "c",
    "fstream": "c",
    "*.tcc": "c",
    "limits": "c",
    "string": "c",
    "vector": "c",
    "exception": "c",
    "istream": "c",
    "ostream": "c",
    "sstream": "c",
    "streambuf": "c",
    "cstdlib": "c",
    "memory": "c",
    "initializer_list": "c",
    "memory_resource": "c",
    "algorithm": "c",
    "cctype": "c",
    "cerrno": "c",
    "clocale": "c",
    "cstdarg": "c",
    "cstddef": "c",
    "cstdio": "c",
    "cwchar": "c",
    "cwctype": "c",
    "deque": "c",
    "iterator": "c",
    "numeric": "c",
    "iosfwd": "c",
    "iostream": "c",
    "new": "c",
    "queue": "c",
    "typeinfo": "c"
  },
  "code-runner.executorMap": {
    // 让RunCode总是运行当前目录的main.c
    "c": "cd $dir && gcc main.c -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
  },
  "files.encoding": "utf8",
  "files.autoGuessEncoding": true,
  "LineCount.excludes": [
    "**/capp/**",
    "**/Android.mk",
    "**/.vscode/**",
    "**/node_modules/**"
  ],
}